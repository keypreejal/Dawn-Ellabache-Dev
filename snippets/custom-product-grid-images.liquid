<style>
ul.product-grid-images {
  list-style: none;
  padding: 0;
  margin: 0;
  display: flex;
  flex-wrap: wrap;
  margin-top: 10px;
  justify-content: space-between;
}
ul.product-grid-images li {
  width: calc(50% - 5px);
}
ul.product-grid-images li img {
  display: block;
  width: 100%;
  height: 375px;
  object-fit: cover;
}
@media (max-width: 1500px) {
    ul.product-grid-images li img {
        height: calc(100vw* 0.25);
    }
}
@media (max-width: 768px) {
    ul.product-grid-images li img {
        height: calc(100vw* 0.48828);
    }
}
</style>
<ul class="product-grid-images {% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
